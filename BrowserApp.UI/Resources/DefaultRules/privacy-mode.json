{
  "id": "00000000-0000-0000-0000-000000000001",
  "name": "Privacy Mode",
  "description": "Blocks common tracking domains (Google Analytics, Facebook Pixel, etc.)",
  "site": "*",
  "enabled": false,
  "priority": 100,
  "rules": [
    {
      "type": "block",
      "match": { "urlPattern": "*google-analytics.com/*" }
    },
    {
      "type": "block",
      "match": { "urlPattern": "*googletagmanager.com/*" }
    },
    {
      "type": "block",
      "match": { "urlPattern": "*facebook.com/tr*" }
    },
    {
      "type": "block",
      "match": { "urlPattern": "*connect.facebook.net/*" }
    },
    {
      "type": "block",
      "match": { "urlPattern": "*doubleclick.net/*" }
    },
    {
      "type": "block",
      "match": { "urlPattern": "*hotjar.com/*" }
    },
    {
      "type": "block",
      "match": { "urlPattern": "*segment.com/*" }
    },
    {
      "type": "block",
      "match": { "urlPattern": "*mixpanel.com/*" }
    },
    {
      "type": "block",
      "match": { "urlPattern": "*amplitude.com/*" }
    },
    {
      "type": "block",
      "match": { "urlPattern": "*clarity.ms/*" }
    }
  ]
}
