{
  "id": "00000000-0000-0000-0000-000000000003",
  "name": "Block Ads",
  "description": "Blocks common advertising networks and ad servers",
  "site": "*",
  "enabled": false,
  "priority": 95,
  "rules": [
    {
      "type": "block",
      "match": { "urlPattern": "*googlesyndication.com/*" }
    },
    {
      "type": "block",
      "match": { "urlPattern": "*googleadservices.com/*" }
    },
    {
      "type": "block",
      "match": { "urlPattern": "*adservice.google.com/*" }
    },
    {
      "type": "block",
      "match": { "urlPattern": "*ads.youtube.com/*" }
    },
    {
      "type": "block",
      "match": { "urlPattern": "*pagead2.googlesyndication.com/*" }
    },
    {
      "type": "block",
      "match": { "urlPattern": "*ad.doubleclick.net/*" }
    },
    {
      "type": "block",
      "match": { "urlPattern": "*adnxs.com/*" }
    },
    {
      "type": "block",
      "match": { "urlPattern": "*amazon-adsystem.com/*" }
    },
    {
      "type": "block",
      "match": { "urlPattern": "*adsrvr.org/*" }
    },
    {
      "type": "block",
      "match": { "urlPattern": "*taboola.com/*" }
    },
    {
      "type": "block",
      "match": { "urlPattern": "*outbrain.com/*" }
    },
    {
      "type": "block",
      "match": { "urlPattern": "*criteo.com/*" }
    },
    {
      "type": "block",
      "match": { "urlPattern": "*rubiconproject.com/*" }
    },
    {
      "type": "block",
      "match": { "urlPattern": "*pubmatic.com/*" }
    }
  ]
}
